{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "rm -rf .nuxt .output && nuxt build",
    "build:netlify": "rm -rf .nuxt .output node_modules/.cache && nuxt build && npm run copy:functions && npm run copy:public",
    "copy:functions": "mkdir -p .netlify/functions && cp -r .netlify/functions-internal/server .netlify/functions/",
    "copy:public": "mkdir -p .output && cp -r dist .output/public",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/image": "^1.11.0",
    "@nuxt/ui": "^3.3.3",
    "@nuxtjs/supabase": "^1.6.2",
    "@pinia/nuxt": "^0.11.2",
    "@prisma/client": "^6.16.1",
    "@supabase/supabase-js": "^2.57.4",
    "@types/nodemailer": "^7.0.1",
    "@types/qrcode": "^1.5.5",
    "eslint": "^9.35.0",
    "firebase": "^12.2.1",
    "firebase-admin": "^13.5.0",
    "nodemailer": "^7.0.6",
    "nuxt": "^4.1.1",
    "pinia": "^3.0.3",
    "qrcode": "^1.5.4",
    "typescript": "^5.9.2",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "prisma": "^6.16.1"
  }
}
